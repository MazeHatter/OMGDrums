<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.monadpad.omgdrums"
          android:versionCode="6"
          android:versionName="1.35">
    <uses-sdk android:minSdkVersion="7"
            android:targetSdkVersion="16"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <application android:label="@string/app_name" android:icon="@drawable/drum"

            >
        <activity android:name="Main"
                  android:label="@string/app_name"

                  android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen"
                  android:screenOrientation="portrait"
                  android:launchMode="singleTask"
                >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity android:name=".GetSketchaTuneActivity"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen"
                  android:screenOrientation="portrait"
                >
        </activity>

        <activity android:name=".GetDrawMusicActivity"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen"
                  android:screenOrientation="portrait"
                >
        </activity>

        <activity android:name=".SavedListActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait"
                >
        </activity>


    </application>
</manifest>
